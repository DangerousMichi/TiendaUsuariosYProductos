 <%- include("templates/header") %>
    <h1>Productos de mi tienda</h1>
    <div class="contenedor">
        <table>
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Nombre</th>
                    <th>Descripci√≥n</th>
                    <th>Precio</th>
                    <th>Stock</th>
                    <th>Editar / Borrar</th>
                </tr>
            </thead>
            <tbody>
                <% productosCorrectos.forEach((producto) => { %>
                    <tr>
                        <td><%= producto.idproducto %></td>
                        <td><%= producto.nombre %></td>
                        <td><%= producto.descripcion %></td>
                        <td><%= producto.precio %></td>
                        <td><%= producto.stock %></td>
                        <td>
                            <a href="/editarProducto/<%= producto.idproducto %>">Editar / </a>
                            <a href="/borrarProducto/<%= producto.idproducto %>">Borrar</a>
                        </td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>

    <%- include("templates/footer") %>